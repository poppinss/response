{
  "name": "@poppinss/response",
  "version": "0.0.0",
  "description": "Wrapper over Node.js res, making it easier to send HTTP responses",
  "main": "index.js",
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "version": "npm run build",
    "lint": "tslint --project tsconfig.json",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile && typedoc --theme markdown --mdHideSources --excludePrivate && git add docs",
    "commit": "git-cz",
    "release": "np"
  },
  "keywords": [
    "adonisjs",
    "res"
  ],
  "author": "virk,poppinss",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/dev-utils": "^1.4.0",
    "@adonisjs/mrm-preset": "^2.0.1",
    "@types/node": "^12.0.1",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "del-cli": "^1.1.0",
    "doctoc": "^1.4.0",
    "husky": "^2.2.0",
    "japa": "^2.0.10",
    "mrm": "^1.2.1",
    "np": "^5.0.1",
    "supertest": "^4.0.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "typedoc": "^0.14.2",
    "typedoc-plugin-external-module-name": "^2.1.0",
    "typedoc-plugin-markdown": "^1.2.0",
    "typescript": "^3.4.5"
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "doctoc README.md --title='## Table of contents' && git add README.md",
      "commit-msg": "node ./node_modules/@adonisjs/mrm-preset/validateCommit/conventional/validate.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@adonisjs/cookie": "^1.1.0",
    "content-disposition": "^0.5.3",
    "destroy": "^1.0.4",
    "etag": "^1.8.1",
    "fresh": "^0.5.2",
    "macroable": "^2.0.2",
    "on-finished": "^2.3.0",
    "vary": "^1.1.2"
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  }
}
